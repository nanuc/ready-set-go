{
    "name": "nanuc/ready-set-go",
    "description": "Boilerplate for Nanuc projects based on TALL stack",
    "license": "MIT",
    "require": {
        "nanuc/ready-set-go": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Nanuc\\ReadySetGo\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nanuc\\ReadySetGo\\ReadySetGoServiceProvider"
            ],
            "aliases": {
                "Loggy": "Nanuc\\ReadySetGo\\Facades\\LReadySetGoFacade"
            }
        }
    }
},
